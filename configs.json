{
  "flow-name": "No Flow",
  "flows": {
    "No Flow": "./lib/flows/no-flow.js"
  },
  "actions": {
    "pre": [
      "status",
      "commit"
    ],
    "post": [
      "helpers",
      "exit"
    ],
    "before": "status",
    "after": ""
  },
  "logs": {
    "DONE": true
  },
  "checkConflictString": true,
  "standard-version": {
    "infile": "CHANGELOG.md",
    "message": "chore(release): %s",
    "firstRelease": false,
    "sign": false,
    "noVerify": false,
    "commitAll": false,
    "silent": false,
    "tagPrefix": "v",
    "scripts": {},
    "skip": {},
    "dryRun": false
  }
}