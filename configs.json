{
  "flow": "no-flow",
  "default": "no-flow",
  "branches": {},
  "actions": [],
  "hooks": {
    "pre": [
      "status",
      "commit"
    ],
    "post": [
      "switch branch",
      "helpers",
      "setup",
      "exit"
    ],
    "before": ["check", "status"],
    "after": [""]
  },
  "logs": {
    "DONE": true
  },
  "checkConflictString": true,
  "standard-version": {
    "infile": "CHANGELOG.md",
    "message": "chore(release): %s",
    "firstRelease": false,
    "sign": false,
    "noVerify": false,
    "commitAll": false,
    "silent": true,
    "tagPrefix": "v",
    "scripts": {},
    "skip": {},
    "dryRun": false
  }
}